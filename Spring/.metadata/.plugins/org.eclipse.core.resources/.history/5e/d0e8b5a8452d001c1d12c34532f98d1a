package com.example.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
@RepuestMapping("/user")
public class UserListController {
	
	/** ユーザー一覧画面を表示　*/
	@GerMapping("/list")
	public String getUserList() {
		//ユーザー一覧画面を表示return "
		return "user/list";
	}
}